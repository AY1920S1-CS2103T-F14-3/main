@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":TypeeParser" as TypeeParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":HistoryManager" as HistoryManager MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> TypeeParser : parseCommand(undo)
activate TypeeParser

create UndoCommand
TypeeParser -> UndoCommand
activate UndoCommand

UndoCommand --> TypeeParser
deactivate UndoCommand

TypeeParser --> LogicManager : u
deactivate TypeeParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoEngagementList()
activate Model

Model -> HistoryManager : undo()
activate HistoryManager
HistoryManager -> HistoryManager :isUndoable()
HistoryManager -> HistoryManager :resetData(ReadOnlyEngagementList)
HistoryManager --> Model :
deactivate HistoryManager

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
