        <StackPane prefHeight="200" prefWidth="200" style="-fx-background-color: white;" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <StackPane prefHeight="200" prefWidth="200" style="-fx-background-color: white;" GridPane.columnIndex="2" GridPane.rowIndex="4" />
